ll add(ll a,ll b,ll mod=hell)
{
	return (a+b)%mod;
}
ll sub(ll a,ll b,ll mod=hell)
{
	return (a-b+mod)%mod;
}
ll mul(ll a,ll b,ll mod=hell)
{
	return (a*b)%mod;
}
]]></content>
